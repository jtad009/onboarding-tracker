{
  "name": "react-typescript",
  "version": "1.0.0",
  "description": "React and TypeScript example starter project",
  "keywords": [
    "typescript",
    "react",
    "starter"
  ],
  "main": "src/index.tsx",
  "dependencies": {
    "@types/node": "^18.7.8",
    "autoprefixer": "^10.4.8",
    "axios": "^0.27.2",
    "nodemon": "^2.0.19",
    "postcss": "^8.4.16",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.3.0",
    "react-scripts": "4.0.3",
    "redux": "^4.2.0",
    "redux-devtools-extension": "^2.13.9",
    "redux-thunk": "^2.4.1",
    "styled-components": "^5.3.5",
    "tailwindcss": "^3.1.8",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "@types/react-redux": "^7.1.24",
    "@types/redux": "^3.6.0",
    "@types/redux-thunk": "^2.1.0",
    "@types/styled-components": "^5.1.26",
    "postcss-cli": "^10.0.0",
    "typescript": "^4.7.4"
  },
  "scripts": {
    "start": "yarn run watch:css && react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "watch:css": "postcss src/style.css -o src/assets/main.css",
    "dev": "yarn run watch:css && nodemon ."
